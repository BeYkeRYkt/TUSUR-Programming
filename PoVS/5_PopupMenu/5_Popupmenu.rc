// Меню
//

IDC_MY5_POPUPMENU MENU
BEGIN
    POPUP "&Файл"
    BEGIN
        MENUITEM "В&ыход",                IDM_EXIT
    END
    POPUP "&Справка"
    BEGIN
        MENUITEM "&О программе ...",           IDM_ABOUT
    END
END


IDR_POPUPMENU MENU DISCARDABLE
BEGIN
    POPUP "TEST"
    BEGIN
        MENUITEM "Test", IDM_TEST
        MENUITEM "Close", IDM_CLOSE
    END
END

